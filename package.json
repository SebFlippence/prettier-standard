{
  "name": "prettier-standard",
  "version": "12.0.1",
  "description": "Prettier and standard brought together!",
  "main": "src/index.js",
  "bin": {
    "prettier-standard": "src/cli.js"
  },
  "keywords": [
    "prettier",
    "eslint",
    "standard"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "Adam Stankiewicz <sheerun@sher.pl>",
  "license": "MIT",
  "repository": "sheerun/prettier-standard",
  "dependencies": {
    "chalk": "2.4.2",
    "execa": "^2.0.4",
    "find-up": "^4.1.0",
    "get-stdin": "^7.0.0",
    "globby": "6.1.0",
    "ignore": "^3.3.7",
    "mri": "^1.1.4",
    "prettierx": "0.7.1"
  },
  "devDependencies": {
    "jest": "24.9.0",
    "mock-fs": "4.10.1"
  },
  "scripts": {
    "format": "node src/cli.js '**/*.js'",
    "test": "jest",
    "test2": "cat test/input.js | NODE_ENV=test node ./src/cli.js > ./test/output.js && export DIFF=\"$(diff ./test/expected.js ./test/output.js)\" && [ -z \"$DIFF\" ] || (echo \"$DIFF\" && exit 1)"
  },
  "files": [
    "src"
  ]
}
