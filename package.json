{
  "name": "prettier-standard",
  "version": "14.0.0",
  "description": "Prettier and standard brought together!",
  "main": "src/index.js",
  "bin": {
    "prettier-standard": "src/cli.js"
  },
  "keywords": [
    "prettier",
    "eslint",
    "standard"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "Adam Stankiewicz <sheerun@sher.pl>",
  "license": "MIT",
  "repository": "sheerun/prettier-standard",
  "dependencies": {
    "chalk": "^2.4.2",
    "diff": "^4.0.1",
    "eslint": "6.2.2",
    "execa": "^2.0.4",
    "find-up": "^4.1.0",
    "get-stdin": "^7.0.0",
    "globby": "^6.1.0",
    "ignore": "^3.3.7",
    "mri": "^1.1.4",
    "multimatch": "^3.0.0",
    "parse-diff": "^0.5.1",
    "prettierx": "0.7.1"
  },
  "devDependencies": {
    "isomorphic-git": "0.64.6",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "tmp": "0.1.0"
  },
  "scripts": {
    "format": "node src/cli.js '**/*.js'",
    "test": "jest",
    "lint-staged": "lint-staged"
  },
  "files": [
    "src"
  ],
  "lint-staged": {
    "**/*.js": [
      "echo",
      "node src/cli.js",
      "git add"
    ]
  }
}
